            <Label.RenderTransform>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform Angle="-90"/>
                    <TranslateTransform/>
                </TransformGroup>
            </Label.RenderTransform>
        </Label>
        <Label x:Name="mwarmup" Content="min" HorizontalAlignment="Left" Margin="421,62,0,7" Foreground="Black" RenderTransformOrigin="0.5,0.5" Padding="0,0,0,0" />
